  {
    "name": "Pimp my Wolt",
    "version": "1.0",
    "description": "Add all noname people to join your meal",
    "manifest_version": 2,
    "permissions": [
      "storage"
    ],
    "options_ui": {
      "page": "set-team-page.html",
      "open_in_tab": false
    },
    "content_scripts": [
      {
        "css": [
          "styles.css"
        ],
        "js": [
          "pimp-my-wolt.js"
        ],
        "matches": [
          "https://wolt.com/*"
        ]
      },
      {
        "css": [
          "styles.css"
        ],
        "js": [
          "pimp-my-cibus.js"
        ],
        "matches": [
          "https://www.mysodexo.co.il/*"
        ],
        "all_frames": true,
        "match_about_blank": true
      }
    ]
  }